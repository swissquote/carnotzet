<FindBugsFilter>
	<Match>
		<Not>
			<Bug pattern="
			NP_PARAMETER_MUST_BE_NONNULL_BUT_MARKED_AS_NULLABLE,
			DM_CONVERT_CASE,
			DMI_RANDOM_USED_ONLY_ONCE,
			RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE,
			RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE,
			SIC_INNER_SHOULD_BE_STATIC_ANON,
			EI_EXPOSE_REP,
			EI_EXPOSE_REP2,
			EI_EXPOSE_STATIC_REP2,
			EI_EXPOSE_BUF2,
			EI_EXPOSE_STATIC_BUF2,
			MC_OVERRIDABLE_METHOD_CALL_IN_CONSTRUCTOR,
			MS_EXPOSE_REP,
			MS_EXPOSE_BUF,
			THROWS_METHOD_THROWS_CLAUSE_BASIC_EXCEPTION,
			THROWS_METHOD_THROWS_RUNTIMEEXCEPTION,
			THROWS_METHOD_THROWS_CLAUSE_THROWABLE"
			/> <!-- https://github.com/spotbugs/spotbugs/issues/756 -->
		</Not>
		<Not>
			<Class name="com.github.swissquote.carnotzet.maven.plugin.HelpMojo"/>
			<Bug pattern="UI_INHERITANCE_UNSAFE_GETRESOURCE"/>
		</Not>
	</Match>
</FindBugsFilter>
